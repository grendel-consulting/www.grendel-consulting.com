{
  "name": "www.grendel-consulting.com",
  "description": "Brochureware site for Grendel Consulting",
  "author": "James Ramirez",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "start": "turbo run start",
    "synth": "turbo run synth",
    "format": "prettier  --ignore-path .gitignore --write \"**/*.{json,ts,tsx,md}\"",
    "ci": "turbo run ci"
  },
  "devDependencies": {
    "esbuild": "0.24.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "3.3.3",
    "turbo": "2.2.3"
  },
  "pnpm": {
    "overrides": {
      "axios": "1.7.7",
      "postcss@<8.4.31": ">=8.4.31",
      "yaml@>=2.0.0-5 <2.2.2": ">=2.2.2",
      "graphql@>=16.3.0 <16.8.1": ">=16.8.1",
      "msgpackr@<1.10.1": ">=1.10.1",
      "es5-ext@>=0.10.0 <0.10.63": ">=0.10.63",
      "webpack-dev-middleware@<=5.3.3": ">=5.3.4",
      "express@<4.19.2": ">=4.19.2",
      "@babel/traverse@<7.23.2": ">=7.23.2",
      "braces@<3.0.3": ">=3.0.3",
      "ws@>=8.0.0 <8.17.1": ">=8.17.1",
      "semver@<5.7.2": ">=5.7.2",
      "semver@>=6.0.0 <6.3.1": ">=6.3.1",
      "micromatch@<4.0.8": ">=4.0.8",
      "sanitize-html@<2.12.1": ">=2.12.1",
