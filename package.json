      "express@<4.20.0": ">=4.20.0",
      "path-to-regexp@<0.1.10": ">=0.1.10",
      "cookie@<0.7.0": ">=0.7.0"
    }
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
